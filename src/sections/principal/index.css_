

#principal #about-me {
    padding: 2rem var(--content);
}

#principal #about-me p {
    margin-bottom: 2rem;
    text-align: justify;
}






/*      Media querys        */
@media screen and (max-width: 768px) {
    #principal #hero {
        padding-bottom: 0;
    }
    
    #principal #hero h1 {
        font-size: 5rem;
    }
    
    #principal #hero h2 {
        font-size: 2rem;
        margin-top: 1rem;
    }

    #principal #hero h2 img {
        width: 2rem;
    }
}